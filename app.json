{
  "expo": {
    "name": "Dog or Cat",
    "slug": "dog-or-cat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "dogorcat",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#b7cece"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#b7cece"
      },
      "package": "com.abramz.dogorcat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./app"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/dog-or-cat/"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "005e41e6-62fc-4c3c-8042-0a74c84c186e"
      }
    }
  }
}
